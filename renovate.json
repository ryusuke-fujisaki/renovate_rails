{
  "extends": ["config:base"],
  "enabledManagers": ["bundler"],
  "semanticCommitType": "fix",
  "automerge": false,
  "packageRules": [
    {
      "managers": ["bundler"],
      "matchDepTypes": ["direct"],
      "allowedVersions": "/^\\d+\\.\\d+\\.\\d+$/",
      "enabled": true
    },
    {
      "managers": ["bundler"],
      "matchDepTypes": ["indirect"],
      "enabled": false
    }
  ],
  "schedule": [
    "* 4 15-21 * 2"
  ],
  "timezone": "Asia/Tokyo"
}
